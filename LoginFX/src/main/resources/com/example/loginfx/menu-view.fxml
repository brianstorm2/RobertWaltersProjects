<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.*?>
<?import javafx.scene.control.*?>
<?import javafx.scene.layout.*?>

<!-- Main Menu Interface -->
<VBox fx:id="menuPage" alignment="CENTER" spacing="20.0" xmlns="http://javafx.com/javafx/11.0.14-internal" xmlns:fx="http://javafx.com/fxml/1" fx:controller="com.example.loginfx.MainController">
    <padding>
        <Insets bottom="20.0" left="20.0" right="20.0" top="20.0"/>
    </padding>
    <Button fx:id="loginMenuButton" onAction="#onLoginMenuButtonClick" text="Login"/>
    <Button fx:id="createAccountButton" onAction="#onCreateAccountButtonClick" text="Create Account"/>
    <Button fx:id="quitButton" onAction="#onQuitButtonClick" text="Quit"/>
</VBox>
